
<form action="/TRAINER/AddLevel" method="post"  class="p-5" >
    <div class="card" style="border-radius: 15px; max-height: 700px;" id="addClass">
      <div class="card-body p-3">

       <h1 class="text-center m-5">add level</h1>

       <div class="">

        <div class="form-outline mb-4">
            <label class="form-label" for="title">Title</label>
            <input type="text" name="title" id="title" class="form-control form-control-lg" required />
          </div>
  
          <div class="form-outline">
            <label class="form-label" for="min">Minimum number of points : </label>
            <input type="number" name="min" id="min" class="form-control form-control-lg" min="2" required />
          </div>
  
          <div class="form-outline">
              <label class="form-label" for="max">Maximum number of points : </label>
              <input type="number" name="max" id="max" class="form-control form-control-lg" max="20" required />
          </div>

       </div>
        <div class="d-flex justify-content-center">
          <button type="submit" class="btn btn-block btn-lg gradient-custom-4 text-body">Submit</button>
        </div>
      </div>
    </div>
  </form>
